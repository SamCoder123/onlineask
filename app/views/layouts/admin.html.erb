<!DOCTYPE html>
<html>
  <head>
    <title>Onlineask</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'admin/admins', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>

  <body style="background-image: url(/crossword.png);">
    <div class="container-fluid" id="wrap">
      <%= render "common/navbar" %>
      <%= render "common/flashes" %>
      <%= render_breadcrumb %>
      <div class="row">
        <%= render "common/admin_sidebar" %>
        <div class="col-md-10">
          <%= yield %>
        </div>
      </div>
      <div class="push"></div>
    </div>
    <div class="footer">
      <%= render "common/footer_index" %>
    </div>

    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= yield :handwrite_javascript %>
  </body>
</html>
