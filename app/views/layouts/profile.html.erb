<!DOCTYPE html>
<html>
  <head>
    <title>Onlineask</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag    'account/users', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body style="background-image: url(/crossword.png);">
    <div class="container-fluid" id="wrap">
      <%= render "common/navbar" %>
      <%= render "common/flashes" %>
      <%= render_breadcrumb %>
      <div class="container">
        <div class="col-md-12 col-xs-12">
            <%= render "account/users/exhibition_navbar" %>
        </div>
        <div class="col-md-4">
            <%= render "account/users/exhibition_sidebar" %>
        </div>
        <div class="col-md-8 pull-left">
          <%= yield %>
        </div>

      </div>
      <div class="row">
        <%= render "common/footer" %>
      </div>
    </div>

    <%= yield :handwrite_javascript %>

  </body>
</html>
