<!DOCTYPE html>
<html>
  <head>
    <%= render_page_title_tag %>
    <%= render_page_description_meta_tag %>
    <%= render_page_keywords_meta_tag %>
    <link rel="shortcut icon" href="/default_image.png">
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'welcome_test', media: 'all', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="background-image: url(/crossword.png);">
    <div class="">
      <%= render "common/navbar_index" %>
      <%= render "common/flashes" %>
      <%= render_breadcrumb %>
      <%= yield %>
      <%= render "common/footer_index" %>
    </div>
  <!-- <%= render "common/footer" %> -->
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <%= yield :handwrite_javascript %>
  </body>
</html>
